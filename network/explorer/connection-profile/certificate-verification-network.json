{
  "name": "certificate-verification-network",
  "version": "1.0.0",
  "client": {
    "tlsEnable": true,
    "adminCredential": {
      "id": "admin",
      "password": "adminpw"
    },
    "enableAuthentication": false,
    "organization": "IssuerMSP",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "channels": {
    "certificate-verification-channel": {
      "peers": {
        "peer0.issuer.example.com": {},
        "peer0.verifier.example.com": {}
      }
    }
  },
  "organizations": {
    "IssuerMSP": {
      "mspid": "IssuerMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/issuer.example.com/users/Admin@issuer.example.com/msp/keystore/priv_sk"
      },
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/issuer.example.com/users/Admin@issuer.example.com/msp/signcerts/Admin@issuer.example.com-cert.pem"
      },
      "peers": [
        "peer0.issuer.example.com"
      ]
    },
    "VerifierMSP": {
      "mspid": "VerifierMSP",
      "adminPrivateKey": {
        "path": "/tmp/crypto/peerOrganizations/verifier.example.com/users/Admin@verifier.example.com/msp/keystore/priv_sk"
      },
      "signedCert": {
        "path": "/tmp/crypto/peerOrganizations/verifier.example.com/users/Admin@verifier.example.com/msp/signcerts/Admin@verifier.example.com-cert.pem"
      },
      "peers": [
        "peer0.verifier.example.com"
      ]
    }
  },
  "orderers": {
    "orderer.example.com": {
      "url": "grpcs://orderer.example.com:7050",
      "tlsCACerts": {
        "path": "/tmp/crypto/ordererOrganizations/example.com/orderers/orderer.example.com/tls/ca.crt"
      }
    }
  },
  "peers": {
    "peer0.issuer.example.com": {
      "url": "grpcs://peer0.issuer.example.com:7051",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/issuer.example.com/peers/peer0.issuer.example.com/tls/ca.crt"
      }
    },
    "peer0.verifier.example.com": {
      "url": "grpcs://peer0.verifier.example.com:9051",
      "tlsCACerts": {
        "path": "/tmp/crypto/peerOrganizations/verifier.example.com/peers/peer0.verifier.example.com/tls/ca.crt"
      }
    }
  }
}